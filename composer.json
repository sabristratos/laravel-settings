{
    "name": "stratos/laravel-settings",
    "description": "A flexible and extensible settings management package for Laravel applications",
    "keywords": [
        "laravel",
        "settings",
        "configuration",
        "preferences",
        "stratos"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mohamed Sabri Ben Chaabane",
            "email": "sabri@stratosdigital.io"
        }
    ],
    "require": {
        "php": "^8.3",
        "illuminate/support": "^12.0",
        "illuminate/database": "^12.0",
        "illuminate/cache": "^12.0",
        "illuminate/encryption": "^12.0",
        "symfony/yaml": "^7.0"
    },
    "require-dev": {
        "orchestra/testbench": "^10.0",
        "pestphp/pest": "^4.0",
        "pestphp/pest-plugin-laravel": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Stratos\\Settings\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Stratos\\Settings\\Tests\\": "tests/",
            "App\\Models\\": "tests/Models/",
            "Database\\Factories\\": "database/factories/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Stratos\\Settings\\Providers\\SettingsServiceProvider"
            ],
            "aliases": {
                "Settings": "Stratos\\Settings\\Facades\\Settings"
            }
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
